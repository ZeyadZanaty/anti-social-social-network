{
  "name": "myUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "onlyfriends": {
      "type": "string"
    },
    "onlyauthenticated": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "friends": {
      "type": "hasMany",
      "model": "myUser",
      "foreignKey": "myUserId",
      "through": "friend",
      "keyThrough": "friendId",
      "options": {
        "nestRemoting": true
      }
    },
    "posts": {
      "type": "hasMany",
      "model": "post",
      "foreignKey": ""
    }
  },
  "acls": [
    {
          "accessType": "READ",
          "principalType": "ROLE",
          "principalId": "$authenticated",
          "permission": "ALLOW"
        }
  ],
  "methods": {}
}
